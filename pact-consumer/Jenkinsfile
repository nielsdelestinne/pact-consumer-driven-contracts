pipeline {
    agent any

    stages {
        stage('Build') {
            steps {

                withMaven(maven: 'mvn'){
                    sh '''
                        cd pact-consumer
                        mvn clean install -DskipTests
                    '''
                }


            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
    }
}